{
    "name": "HKDeath/Yaf-Swoole",
    "description": "The Yaf-Swoole Framework.",
    "keywords": ["framework", "Yaf-Swoole"],
    "license": "MIT",
    "type": "project",
    "require": {
        "catfan/Medoo": "^1.5",
        "filp/whoops": "^2.1",
        "symfony/var-dumper": "^4.0",
        "monolog/monolog": "^1.23",
        "fangstar/php-validator": "^1.0",
        "phpmailer/phpmailer": "^6.0",
        "phpoffice/phpexcel": "^1.8",
        "crada/php-apidoc": "~1.3",
        "predis/predis": "^1.1.1",
        "hightman/xunsearch": "^1.4"
    },

    "autoload": {
        "psr-4": {
            "Library\\": "app/library",
            "Model\\": "app/models",
            "Widget\\": "app/widgets",
            "Common\\": "app/common",
            "Exception\\" : "app/exception"
        },
        "files": [
            "app/library/functions.php"
        ]
    }
}
